    "json-bigint": "^0.3.0",
    "lodash": "4.17.15",
    "markdown-it": "^8.4.2",
    "moment": "^2.22.2",
    "npm": "6.10.2",
    "numeral": "^2.0.6",
    "quill": "^1.3.6",
    "resolve-url-loader": "3.1.0",
